.tab-bar-wrap {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 999999;
  width: 100%;
  height: 98rpx;
  background-color: #f7f7fa;
  border-top: 1px solid #eee;
}

.tab-bar {
  width: 100%;
  height: 98rpx;
  background-color: #f7f7fa;
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.tabBarIco{ 
	width: 40rpx;
	height: 40rpx;
	display: block;  
	margin: 6rpx auto 0 auto;  
  }
.tabBar-item{height: 98rpx;flex: 1;text-align: center;}
.tabbar-txt{display: block;font-size: 32rpx;color: #333;margin-top: 4rpx;}
.curPage{color: #1296db;}