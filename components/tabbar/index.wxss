.tab-bar-wrap {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 999999;
  width: 100%;
  height: 98rpx;
  background-color: #f7f7fa;
}

.tab-bar {
  width: 100%;
  height: 98rpx;
  background-color: #f7f7fa;
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
